@model IEnumerable<System.ServiceModel.Syndication.SyndicationItem>

<h2>News</h2>
@foreach (var feed in Model)
            {
    <h4>
        @Html.DisplayFor(modelItem => feed.Title.Text)
    </h4>
    @Html.DisplayFor(modelItem => ((System.ServiceModel.Syndication.TextSyndicationContent)feed.Content).Text)
}
